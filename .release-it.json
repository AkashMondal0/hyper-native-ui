{
  "npm": {
    "publish": true
  },
  "git": {
    "commitMessage": "chore: release ${version}",
    "requireBranch": "main",
    "tagName": "v${version}",
    "push": true
  },
  "github": {
    "release": false
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "infile": "CHANGELOG.md"
    }
  }
}